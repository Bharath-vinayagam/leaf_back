{
  "version": 2,
  "functions": {
    "api/index.py": { "runtime": "python3.11" }
  },
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "^/$", "dest": "api/index.py" },
    { "src": "^/(predict|detect-leaf|docs|openapi.json)$", "dest": "api/index.py" },
    { "src": "/api/(.*)", "dest": "api/index.py" }
  ]
}


